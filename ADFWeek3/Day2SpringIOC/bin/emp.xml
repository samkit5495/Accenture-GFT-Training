<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id="a" class="com.accenture.day2.springIOC.model.Address">
		<property name="pincode" value="411041"></property>
		<property name="city" value="Pune"></property>
	</bean>
	
	<bean id="e" class="com.accenture.day2.springIOC.model.Employee">
		<property name="id" value="114"></property>
		<property name="name" value="Samkit"></property>
		<property name="address" ref="a"></property>
	</bean>
	
</beans>	
